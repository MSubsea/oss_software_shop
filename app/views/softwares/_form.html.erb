<div class="container mt-5">
  <%= simple_form_for(@software) do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :photo, as: :file %>
    <%= f.input :customer_id, collection: @collection, selected: @selected %>
    <%= f.submit %>
  <% end %>
</div>
